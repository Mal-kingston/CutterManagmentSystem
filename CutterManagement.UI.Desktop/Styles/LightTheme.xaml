<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <!--#region Colors-->

    <!--  light theme background color palette  -->
    <Color x:Key="BackgroundColor1">#ffffff</Color>
    <Color x:Key="BackgroundColor2">#f6f7f9</Color>
    <Color x:Key="BackgroundColor3">#e8ebee</Color>
    <Color x:Key="BackgroundColor4">#eaeaea</Color>
    <Color x:Key="BackgroundColor5">#3d6cf6</Color>
    <Color x:Key="BackgroundColor6">#f7d8d6</Color>
    <Color x:Key="BackgroundColor7">#4e4e64</Color>

    <!--  success | warning | error -> backgroung  -->
    <Color x:Key="SuccessBackgroundColor">#def2d6</Color>
    <Color x:Key="WarningBackgroundColor">#f8f3d6</Color>
    <Color x:Key="ErrorBackgroundColor">#ebc8c4</Color>

    <!--  light theme foreground color palette  -->
    <Color x:Key="ForegroundColor1">#000000</Color>
    <Color x:Key="ForegroundColor2">#bbbbbb</Color>
    <Color x:Key="ForegroundColor3">#4e4e64</Color>
    <Color x:Key="ForegroundColor4">#3d6cf6</Color>
    <Color x:Key="ForegroundColor5">#f62b4f</Color>
    <Color x:Key="ForegroundColor6">#8f8f94</Color>
    <Color x:Key="ForegroundColor7">#ffffff</Color>
    <Color x:Key="ForegroundColor8">#a05859</Color>

    <!--  success | warning | error -> foreground  -->
    <Color x:Key="SuccessForegroundColor">#586c50</Color>
    <Color x:Key="SuccessIconForegroundColor">#adc1a5</Color>
    <Color x:Key="WarningForegroundColor">#937d4b</Color>
    <Color x:Key="WarningIconForegroundColor">#d9d5a5</Color>
    <Color x:Key="ErrorForegroundColor">#b63947</Color>
    <Color x:Key="ErrorIconForegroundColor">#ca9b9b</Color>

    <!--  light theme background color palette on hover  -->
    <Color x:Key="ButtonHoverBackgroundColor1">#4e4e64</Color>

    <!--  light theme border color palette  -->
    <Color x:Key="BorderColor1">#e6e8e9</Color>
    <Color x:Key="BorderColor2">#edefef</Color>
    <Color x:Key="BorderColor3">#3d6cf6</Color>
    <Color x:Key="BorderColor4">#f62b4f</Color>
    <Color x:Key="BorderColor5">#4e4e64</Color>
    <Color x:Key="BorderColor6">#bbbbbb</Color>

    <!--  accent color  palette  -->
    <Color x:Key="AccentColor1">#ffff00</Color>
    <Color x:Key="AccentColor2">#ff6b6c</Color>
    <Color x:Key="AccentColor3">#5cffaa</Color>
    <Color x:Key="AccentColor4">#ffb519</Color>
    <Color x:Key="AccentColor5">#24b17c</Color>
    <Color x:Key="AccentColor6">#a0a0a0</Color>
    <Color x:Key="AccentColor7">#dddddd</Color>

    <!--#endregion-->

    <!--#region Brushes-->

    <!--  light theme background color brush palette  -->
    <SolidColorBrush x:Key="BackgroundColorBrush1" Color="{StaticResource BackgroundColor1}" />
    <SolidColorBrush x:Key="BackgroundColorBrush2" Color="{StaticResource BackgroundColor2}" />
    <SolidColorBrush x:Key="BackgroundColorBrush3" Color="{StaticResource BackgroundColor3}" />
    <SolidColorBrush x:Key="BackgroundColorBrush4" Color="{StaticResource BackgroundColor4}" />
    <SolidColorBrush x:Key="BackgroundColorBrush5" Color="{StaticResource BackgroundColor5}" />
    <SolidColorBrush x:Key="BackgroundColorBrush6" Color="{StaticResource BackgroundColor6}" />
    <SolidColorBrush x:Key="BackgroundColorBrush7" Color="{StaticResource BackgroundColor7}" />

    <!--  light theme foreground color brush palette  -->
    <SolidColorBrush x:Key="ForegroundColorBrush1" Color="{StaticResource ForegroundColor1}" />
    <SolidColorBrush x:Key="ForegroundColorBrush2" Color="{StaticResource ForegroundColor2}" />
    <SolidColorBrush x:Key="ForegroundColorBrush3" Color="{StaticResource ForegroundColor3}" />
    <SolidColorBrush x:Key="ForegroundColorBrush4" Color="{StaticResource ForegroundColor4}" />
    <SolidColorBrush x:Key="ForegroundColorBrush5" Color="{StaticResource ForegroundColor5}" />
    <SolidColorBrush x:Key="ForegroundColorBrush6" Color="{StaticResource ForegroundColor6}" />
    <SolidColorBrush x:Key="ForegroundColorBrush7" Color="{StaticResource ForegroundColor7}" />
    <SolidColorBrush x:Key="ForegroundColorBrush8" Color="{StaticResource ForegroundColor8}" />

    <!--  light theme background color palette on hover  -->
    <SolidColorBrush x:Key="ButtonHoverBackgroundColorBrush1" Color="{StaticResource ButtonHoverBackgroundColor1}" />

    <!--  light theme border color brush palette  -->
    <SolidColorBrush x:Key="BorderColorBrush1" Color="{StaticResource BorderColor1}" />
    <SolidColorBrush x:Key="BorderColorBrush2" Color="{StaticResource BorderColor2}" />
    <SolidColorBrush x:Key="BorderColorBrush3" Color="{StaticResource BorderColor3}" />
    <SolidColorBrush x:Key="BorderColorBrush4" Color="{StaticResource BorderColor4}" />
    <SolidColorBrush x:Key="BorderColorBrush5" Color="{StaticResource BorderColor5}" />
    <SolidColorBrush x:Key="BorderColorBrush6" Color="{StaticResource BorderColor6}" />

    <!--  light theme accent color brush palette  -->
    <SolidColorBrush x:Key="AccentColorBrush1" Color="{StaticResource AccentColor1}" />
    <SolidColorBrush x:Key="AccentColorBrush2" Color="{StaticResource AccentColor2}" />
    <SolidColorBrush x:Key="AccentColorBrush3" Color="{StaticResource AccentColor3}" />
    <SolidColorBrush x:Key="AccentColorBrush4" Color="{StaticResource AccentColor4}" />
    <SolidColorBrush x:Key="AccentColorBrush5" Color="{StaticResource AccentColor5}" />
    <SolidColorBrush x:Key="AccentColorBrush6" Color="{StaticResource AccentColor6}" />
    <SolidColorBrush x:Key="AccentColorBrush7" Color="{StaticResource AccentColor7}" />

    <!--  success | warning | error -> backgroung  -->
    <SolidColorBrush x:Key="SuccessBackgroundColorBrush" Color="{StaticResource SuccessBackgroundColor}" />
    <SolidColorBrush x:Key="WarningBackgroundColorBrush" Color="{StaticResource WarningBackgroundColor}" />
    <SolidColorBrush x:Key="ErrorBackgroundColorBrush" Color="{StaticResource ErrorBackgroundColor}" />

    <!--  success | warning | error -> foreground  -->
    <SolidColorBrush x:Key="SuccessForegroundColorBrush" Color="{StaticResource SuccessForegroundColor}" />
    <SolidColorBrush x:Key="SuccessIconForegroundColorBrush" Color="{StaticResource SuccessIconForegroundColor}" />
    <SolidColorBrush x:Key="WarningForegroundColorBrush" Color="{StaticResource WarningForegroundColor}" />
    <SolidColorBrush x:Key="WarningIconForegroundColorBrush" Color="{StaticResource WarningIconForegroundColor}" />
    <SolidColorBrush x:Key="ErrorForegroundColorBrush" Color="{StaticResource ErrorForegroundColor}" />
    <SolidColorBrush x:Key="ErrorIconForegroundColorBrush" Color="{StaticResource ErrorIconForegroundColor}" />

    <!--#endregion-->

</ResourceDictionary>