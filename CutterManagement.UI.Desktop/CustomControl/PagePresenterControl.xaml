<UserControl x:Class="CutterManagement.UI.Desktop.PagePresenterControl" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:CutterManagement.UI.Desktop"
             d:DesignHeight="450" d:DesignWidth="800"
             mc:Ignorable="d">

    <Grid>
        <Frame VerticalAlignment="Stretch" HorizontalContentAlignment="Stretch"
               local:ViewLocator.FindView="{Binding CurrentPage}"
               NavigationUIVisibility="Hidden" />
    </Grid>
</UserControl>
